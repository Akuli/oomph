import "types.oomph" as types

export union Instruction:
    FunctionCall
    StringConstant

export class FunctionCall(types::BuiltinVariable function, List[types::LocalVariable] args)
export class StringConstant(Str value, types::LocalVariable res)

export class FuncDef(types::ThisFileVariable var, List[types::LocalVariable] argvars, List[Instruction] body)
