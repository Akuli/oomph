func show(optional[Str] x):
    print("show: {x} {x.is_null()}")

export func main():
    let x = new optional[Str]("lol" + "wat")
    print("main: {x} {x.is_null()}")
    show(x)
    show(null[Str])

    print(null[Str] == null[Str])
    print(x == null[Str])
    print(x == new optional[Str]("lolwat"))
