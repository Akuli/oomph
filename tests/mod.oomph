export func main():
    print(7 mod 5)
    print(2 mod 5)
    print(-3 mod 5)
    print(-8 mod 5)

    print(7 mod -5)
    print(2 mod -5)
    print(-3 mod -5)
    print(-8 mod -5)

    print(0 mod 5)
    print(0 mod -5)

    print(1 mod 2.5)
    print(-1.5 mod 2.5)
    print(1 mod -2.5)
    print(-1.5 mod -2.5)

    # This is a syntax error without extra parentheses, see mod_error.oomph
    print(4 == (1 mod 3))
    print(1 == (4 mod 3))
