import "hello_lib.oomph" as hello
import "<stdlib>/hello.oomph" as hello_std

export func main():
    hello::print_hello()
    hello_std::print_hello()

    let b = new hello::Bar(123, new optional[Str]("lol"))
    print(b)
    print(b.thing)
    print(b.optional_thing)
    b.lol()

    let u = new hello::FooBar(b)
    print(u)
    switch u:
        case hello::Foo:
            print("Foo from switch: {u}")
        case hello::Bar:
            print("Bar from switch: {u}")
