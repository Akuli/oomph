func main():
    print(1 == 1)
    print(1 == 2)
    print(1.0 == 1)
    print(0.1 + 0.2 == 0.3)   # false, just like in most other programming languages
    print(true == true)
    print(true == false)
    print(false == false)
    print(false == true)

    print(1 != 1)
    print(1 != 2)
    print(1.0 != 1)
    print(0.1 + 0.2 != 0.3)
    print(true != true)
    print(true != false)
    print(false != false)
    print(false != true)
