union Thing:
    Foo
    Bar
    Baz

class Foo(int foo_thing)
class Bar()
class Baz()

func print_thing(Thing thing) -> void:
    switch thing as t:   # TODO: switch to plain 'switch thing'
        case Foo:
            print("Foo {t.foo_thing}")
        case Bar:
            print("Bar")
        case Baz:
            print("Baz")

func main() -> void:
    print_thing(new Thing(new Foo(69)))
    print_thing(new Thing(new Bar()))
    print_thing(new Thing(new Baz()))
