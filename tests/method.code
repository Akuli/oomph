class Point(int x, int y):
    meth print() -> void:
        print_int(self.x)
        print_int(self.y)
    meth add(Point other) -> void:
        new Point(add(self.x, other.x), add(self.y, other.y)).print()


func main() -> void:
    let p = new Point(100, 200)
    p.print()
    p.add(new Point(1, 2))
