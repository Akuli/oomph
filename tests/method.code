class Point(int x, int y):

    meth print() -> void:
        print(self.x)
        print(self.y)

    meth add(Point other) -> Point:
        return new Point(self.x + other.x, self.y + other.y)


func main() -> void:
    new Point(100, 200).print()
    let p = new Point(100, 200)
    p.add(new Point(1, 2)).print()
