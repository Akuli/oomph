import "<stdlib>/process.oomph" as process

export func main():
    # FIXME: prints between commands appear after command output (missing flushing?)
    let statuses = [
        process::run(["./oomphc", "--help"]),
        process::run(["./oomphc"]),
        process::run(["./oomphc", "--lolwat"]),
        process::run(["./oomphc", "-o"]),
        process::run(["./oomphc", "--outfile"]),
        process::run(["./oomphc", "--valgrind"]),
    ]
    print("Exit statuses: {statuses}")
