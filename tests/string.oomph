export func main():
    print("hello" + " " + "world")
    print("hello" == "hello")
    print("hello" == "hell")
    print("hello" == "hallo")

    print("'")

    print("Length:")
    print("hello".length())
    print("hellö".length())
    print("".length())

    print("UTF-8:")
    print("hello".get_utf8())
    print("hellö".get_utf8())
    print("".get_utf8())

    print("Repeats:")
    print("abc".repeat(-5))    # TODO: should negative repeats be disallowed?
    print("abc".repeat(0))
    print("abc".repeat(1))
    print("abc".repeat(2))
    print("abc".repeat(3))
    print("abc".repeat(4))
    print("abc".repeat(5))

    print("'in' operator:")
    print("a" in "abc")
    print("x" in "abc")
    print("abc" in "abc")
    print("abcd" in "abc")
    print("" in "abc")
    print("a" in "")
    print("" in "")

    print("Starts with:")
    print("abc".starts_with("ab"))
    print("abc".starts_with("bc"))
    print("abc".starts_with("abc"))
    print("abc".starts_with(""))
    print("abc".starts_with("abcx"))
    print("abc".starts_with("xabc"))

    print("Ends with:")
    print("abc".ends_with("ab"))
    print("abc".ends_with("bc"))
    print("abc".ends_with("abc"))
    print("abc".ends_with(""))
    print("abc".ends_with("abcx"))
    print("abc".ends_with("xabc"))

    print("Pad:")
    print("abc".left_pad(5, "*"))
    print("abc".right_pad(5, "*"))
    print("abc".center_pad(5, "*"))
    print("abc".center_pad(6, "*"))

    print("Count:")
    print("abclolwatlolabc".count("lol"))
    print("aaaa".count("aa"))
    print("aaaa".count("b"))
    print("aaaa".count(""))

    print("Trim:")
    print(" a ".left_trim())
    print(" a ".right_trim())
    print(" a ".trim())

    print("Replace:")
    print("abcab".replace("ab", "x"))
    print("aaaaa".replace("aa", "x"))
    print("aaaa".replace("aa", "x"))
    print("aaaa".replace("aa", "xxxx"))
    print("asdf".replace("", "x"))
