func main() -> void:
    print("hello" + " " + "world")
    print("hello" == "hello")
    print("hello" == "hell")
    print("hello" == "hallo")

    print("'")

    print("hello".length())
    print("hello".unicode_length())
    print("hellö".length())
    print("hellö".unicode_length())
    print("".length())
    print("".unicode_length())

    print("hello".slice(9, -6))
    print("hello".slice(2, 2))

    print("hello".slice(0, 5))
    print("hello".slice(0, 2))
    print("hello".slice(2, 5))
    print("hello".slice(-6, 9))

    # TODO: should negative repeats be disallowed?
    print("abc".repeat(-5))
    print("abc".repeat(0))
    print("abc".repeat(1))
    print("abc".repeat(2))
    print("abc".repeat(3))
    print("abc".repeat(4))
    print("abc".repeat(5))

    print("abc".starts_with("ab"))
    print("abc".starts_with("bc"))
    print("abc".starts_with("abc"))
    print("abc".starts_with(""))
    print("abc".starts_with("abcx"))
    print("abc".starts_with("xabc"))

    print("abc".ends_with("ab"))
    print("abc".ends_with("bc"))
    print("abc".ends_with("abc"))
    print("abc".ends_with(""))
    print("abc".ends_with("abcx"))
    print("abc".ends_with("xabc"))
