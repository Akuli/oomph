export func main():
    let a = new Mapping[Str, Int]()
    let b = new Mapping[Str, Int]()

    print("First key:")
    print(a == b)
    print(b == a)
    a.set("x", 1)
    print(a == b)
    print(b == a)
    b.set("x", 2)
    print(a == b)
    print(b == a)
    b.set("x", 1)
    print(a == b)
    print(b == a)

    print("Second key:")
    a.set("y", 2)
    print(a == b)
    print(b == a)
    b.set("z", 2)
    print(a == b)
    print(b == a)
    b.delete("z")
    print(a == b)
    print(b == a)
    b.set("y", 2)
    print(a == b)
    print(b == a)

    print("Third key:")
    a.set("z", 3)
    print(a == b)
    print(b == a)
    b.set("z", 6)
    print(a == b)
    print(b == a)
    b.set("z", 3)
    print(a == b)
    print(b == a)
