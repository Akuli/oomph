func show(optional[Str] x) -> void:
    if x.is_null():
        print("show: null")
    else:
        print("show: {x.get()}")

func main() -> void:
    let x = new optional[Str]("lol" + "wat")
    if x.is_null():
        print("main: null")
    else:
        print("main: {x.get()}")

    show(x)
    show(null[Str])
